Dinstall
{
  Options
  {
	Automatic "";
        Debug "";
        Help "";
        Ack-New "";
        Manual-Reject "";
        No-Action "";
        No-Lock "";
        No-Version-Check "";
        No-Mail "";
        Override-Distribution "";
