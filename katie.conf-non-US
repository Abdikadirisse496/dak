  Unstable
  {
	Components 
	{
	  non-US/main;
	  non-US/contrib;
	  non-US/non-free;
	};
	Architectures 
	{
	  source; 
	  all;
	  alpha; 
	  arm;
	  hppa;
	  hurd-i386;
	  i386;
	  ia64;
	  m68k;
	  mips;
	  mipsel;
	  powerpc;
	  s390;
	  sh;
	  sparc;
	};
	Announce "debian-devel-changes@lists.debian.org";
	Origin "Debian";
	Description "Debian Unstable - Not Released";
	CodeName "sid";
	OverrideCodeName "sid";
	Priority "5";
	VersionChecks
	{
	  MustBeNewerThan
	  {
	    Stable;
	    Proposed-Updates;
	    Testing;
	    Woody-Proposed-Updates;
	  };
	};
  };

