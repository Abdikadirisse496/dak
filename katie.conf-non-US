Suite
{

  // Priority determines which suite is used for the Maintainers filed as generated by charisma/da_mkmaintainers (highest wins)
  
  Experimental
  {
	Architectures 
	{
	  "source" "";  
	  "all" "";
	  "alpha" ""; 
	  "arm" "";
	  "i386" "";
	  "m68k" "";
	  "powerpc" "";
	  "sparc" "";
	};
